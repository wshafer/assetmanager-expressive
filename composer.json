{
    "name": "wshafer/assetmanager-expressive",
    "description": "An assetmanager module for Zend Expressive.",
    "license": "BSD-2-Clause",
    "type": "library",
    "keywords": [
        "asset",
        "asset manager",
        "expressive"
    ],
    "homepage": "https://github.com/wshafer/assetmanager-expressive",
    "authors": [
        {
            "name": "Wesley Overdijk",
            "email": "r.w.overdijk@gmail.com",
            "homepage": "http://blog.spoonx.nl/"
        },
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        },
        {
            "name": "Westin Shafer",
            "email": "shafer_w2002@yahoo.com",
            "homepage": "https://github.com/wshafer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.7.0",
        "zendframework/zend-expressive": "^2.0.2",
        "wshafer/assetmanager-core": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8",
        "squizlabs/php_codesniffer": "~3.0",
        "phpmd/phpmd" : "@stable"
    },
    "autoload": {
        "psr-4": {
            "AssetManager\\Expressive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssetManager\\Expressive\\Test\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "config-provider": "AssetManager\\Expressive\\Module"
        }
    }
}
